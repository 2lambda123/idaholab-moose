[Tests]
  [./basic]
    type = 'Exodiff'
    input = 'reportable_kernel.i'
    exodiff = 'reportable_kernel_out.e'
  [../]

	[./by_name]
    type = 'Exodiff'
    input = 'reportable_kernel.i'
    exodiff = 'reportable_kernel_by_name_out.e'
		cli_args = 'Kernels/diff/test=by_name Output/file_base=reportable_kernel_by_name_out'
  [../]

  [./has_reportable]
    type = 'RunException'
    input = 'reportable_kernel.i'
    expect_err = "The name 'unknown' is not a reportable value"
		cli_args = 'Kernels/diff/test=has_reportable'
  [../]

  [./disable_output]
    type = 'Exodiff'
    input = 'reportable_kernel.i'
    exodiff = 'reportable_kernel_no_report_out.e'
		cli_args = 'Kernels/diff/report=false Output/file_base=reportable_kernel_no_report_out'
  [../]

	[./transient] # runs a test with 20 steps
    type = 'Exodiff'
		input = 'reportable_transient.i'
		exodiff = 'reportable_transient_out.e'
  []

	[./restart1] # runs 10 steps of transient
		type = 'Exodiff'
		input = 'reportable_restart1.i'
		exodiff = 'reportable_restart1_out.e'
	[../]

	[./restart2] # restarts and runs another 10 steps of transient, results should match ./transient test
		type = 'Exodiff'
		input = 'reportable_restart2.i'
		exodiff = 'reportable_restart2_out.e'
		prereq = 'restart1 transient'
	[../]
[]