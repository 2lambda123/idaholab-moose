    input = 'console.i'
    cli_args = 'Outputs/screen/output_file=true Outputs/screen/file_base=console_file_system_information_out'
    check_files = 'console_file_system_information_out.txt'
    file_expect_out = 'Num\s*DOFs:\s*242'
