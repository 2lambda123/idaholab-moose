[Tests]
  # physics
  [./phy:f_fn_3eqn]
    type = 'CSVDiff'
    input = 'phy.f_fn.3eqn.i'
    csvdiff = 'phy.f_fn.3eqn_out.csv'
    rel_err = 1e-10
  [../]

  [./phy:par_fn]
    type = 'CSVDiff'
    input = 'phy.par_fn.i'
    csvdiff = 'phy.par_fn_out.csv'
    rel_err = 1e-10
  [../]

  [./phy:sub_discretization]
    type = 'Exodiff'
    input = 'phy.sub_discretization.i'
    exodiff = 'phy.sub_discretization_out.e'
    custom_cmp = 'phy.sub_discretization.exodiff'
    # matching geometry at t=0
    recover = false
  [../]

  # controls

  # error checking
  [./err:wrong_fp]
    type = 'RunException'
    input = 'err.wrong_fp.i'
    expect_err = "pipe: Supplied fluid properties must be for 1-phase fluids."
  [../]

  [./err:wrong_ht_T_wall]
    type = 'RunException'
    input = 'err.wrong_ht.i'
    expect_err = "pipe: Coupled heat source 'ht_pipe' is not compatible with single phase flow channel\. Use single phase heat transfer component instead\."
    cli_args = 'Components/ht_pipe/type=HeatTransferFromSpecifiedTemperature2Phase Components/ht_pipe/T_wall=300'
  [../]

  # jacobians
  [./jac:3eqn]
    type = 'PetscJacobianTester'
    input = 'jac.3eqn.i'
    ratio_tol = 7e-8
    difference_tol = 15
    cli_args = 'Debug/check_jacobian=true'
  [../]
