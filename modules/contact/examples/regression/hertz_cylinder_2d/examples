  [cylinder_friction_mortar]
    type = CSVDiff
    input = 'cylinder_friction_mortar.i'
    csvdiff = 'cylinder_friction_mortar_chkfile_friction_0002.csv'
    abs_zero = 5.0
    rel_err = 1.0e-3
    requirement = 'Contact module shall compute mortar frictional forces according to reference solution '
                  'when a cylinder is pressed onto a rigid plane. Similar reference solution can be '
                  'consulted in section 3.6 of 2008, Hueber, Discretization techniques and efficient '
                  'algorithms for contact problems. '
    max_time = 4800
    min_parallel = 2
    max_parallel = 8
  []
