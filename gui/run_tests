#!/usr/bin/env python
import sys, os, inspect, importlib

import src

class PeacockTester(object):
  def __init__(self):
    pass


for root, dirs, files in os.walk('tests'):
  for f in files:
    if f.endswith('.py') and f != '__init__.py':
      mod, ext = os.path.splitext(os.path.join(root))
      mod = mod.replace('/','.')
      print 'Loading:', mod
      i = importlib.import_module(mod)


      print i.testDataRead()
